<h1>나의 카테고리</h1>
<div id="myCategoryList" class="myCategoryList">
    <% myCategory.map((data)=>{ %>
        <div class="categoryList">
            <p><%= data.name %></p>
            <div class="categoryIcon"><i onclick="categoryEditModalOpen('<%=data._id%>')" class="fa-regular fa-pen-to-square"></i>
                <i class="fa-regular fa-trash-can" onclick="categoryDelete('<%=data._id%>', '<%=data.name%>')"></i></div>
        </div>
    <% }) %>
</div>