<!--
    담당자 : 박신욱
    주요 기능 : 지도 API를 사용하여 사용자가 검색하여 지정한 위치를 보여 줍니다.
 -->
<link rel="stylesheet" href="/stylesheets/schedule/kakaoMap.css">
<div id="map_wrap" class="map_wrap" style="display: none;">
    <div id="map" style="width:100%;height:100%;position:relative;overflow:hidden;"></div>
    <div id="menu_wrap" class="bg_white">
        <div class="option">
            <div>
                <form onsubmit="searchPlaces(); return false;">
                    <input type="text" value="" id="keyword" size="15">
                    <button type="submit" id="keyword_btn">검색</button>
                </form>
            </div>
        </div>
        <hr>
        <ul id="placesList"></ul>
        <div id="pagination"></div>
    </div>
</div>
<script type="text/javascript" src="//dapi.kakao.com/v2/maps/sdk.js?appkey=<%= kakaoKey %>&libraries=services"></script>

